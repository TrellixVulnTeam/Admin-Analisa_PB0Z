AAzBH,IAAA,KAAK,EAAA,CAAC,YAAD,EAAa,CACT,MAAA,CAAA,IAAA,yBAAsB,MAAA,CAAA,IAAtB,CADS,EACqB,MAAA,CAAA,KAAA,8BAA4B,MAAA,CAAA,KAA5B,CADrB,CAAb;AAyBF,GA1BL,E,CAIE,YAAA,CAMK,IANL,EAMK;AAND,IAAA,KAAK,EAAA,CAAC,qBAAD,EAAsB;AAAA,MAAA,QAAA,EAAqB,MAAA,CAAA,KAAA,KAAK;AAA1B,KAAtB;AAMJ,GANL,E,CACE,YAAA,CAII,GAJJ,EAII;AAJD,IAAA,KAAK,EAAC,WAIL;AAJiB,kBAAW,UAI5B;AAJwC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,QAAA,IAAA,QAAA,CAAA,QAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAI7C,GAJJ,E,CACE,U,CADF,C,CADF,E;;AAAA,G,qBAOA,YAAA,CAOK,SAPL,EAOK,IAPL,EAOK,WAAA,CAHY,QAAA,CAAA,KAAA,CAAM,QAAA,CAAA,OAAN,EAAe,QAAA,CAAA,OAAf,CAGZ,EAHkC,UAA9B,IAA8B,EAA1B;yBAJb,YAAA,CAOK,IAPL,EAOK;AANH,MAAA,KAAK,EAAA,CAAC,WAAD,EAAY;AAAA,QAAA,MAAA,EACC,MAAA,CAAA,KAAA,KAAU;AADX,OAAZ,CAMF;AAJF,MAAA,GAAG,EAAE;AAIH,KAPL,E,CAME,YAAA,CAA6D,GAA7D,EAA6D;AAA1D,MAAA,KAAK,EAAC,WAAoD;AAAvC,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,UAAA,CAAW,IAAX,CAAF;AAAA;AAAkC,KAA7D,E,iBAAkD,I,CAAlD,EAAsD;AAAA;AAAtD,MAAsD,CAAA,SAAA,CAAtD,C,CANF,E;;AAAA,K;GAOK,CAPL,E;;AAAA,G,GAQA,YAAA,CAMK,IANL,EAMK;AAND,IAAA,KAAK,EAAA,CAAC,qBAAD,EAAsB;AAAA,MAAA,QAAA,EAAqB,MAAA,CAAA,KAAA,KAAU,QAAA,CAAA;AAA/B,KAAtB;AAMJ,GANL,E,CACE,YAAA,CAII,GAJJ,EAII;AAJD,IAAA,KAAK,EAAC,WAIL;AAJiB,kBAAW,MAI5B;AAJoC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,QAAA,IAAA,QAAA,CAAA,QAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAIzC,GAJJ,E,CACE,U,CADF,C,CADF,E;;AAAA,G,CAnBF,E;;AAAA,G","sourcesContent":["<template>\n  <ul\n    class=\"pagination\"\n    :class=\"[size && `pagination-${size}`, align && `justify-content-${align}`]\"\n  >\n    <li class=\"page-item prev-page\" :class=\"{ disabled: value === 1 }\">\n      <a class=\"page-link\" aria-label=\"Previous\" @click=\"prevPage\">\n        <span aria-hidden=\"true\"\n          ><i class=\"fa fa-angle-left\" aria-hidden=\"true\"></i\n        ></span>\n      </a>\n    </li>\n    <li\n      class=\"page-item\"\n      :class=\"{ active: value === item }\"\n      :key=\"item\"\n      v-for=\"item in range(minPage, maxPage)\"\n    >\n      <a class=\"page-link\" @click=\"changePage(item)\">{{ item }}</a>\n    </li>\n    <li class=\"page-item next-page\" :class=\"{ disabled: value === totalPages }\">\n      <a class=\"page-link\" aria-label=\"Next\" @click=\"nextPage\">\n        <span aria-hidden=\"true\"\n          ><i class=\"fa fa-angle-right\" aria-hidden=\"true\"></i\n        ></span>\n      </a>\n    </li>\n  </ul>\n</template>\n<script>\nexport default {\n  name: \"base-pagination\",\n  props: {\n    pageCount: {\n      type: Number,\n      default: 0,\n      description:\n        \"Pagination page count. This should be specified in combination with perPage\",\n    },\n    perPage: {\n      type: Number,\n      default: 10,\n      description:\n        \"Pagination per page. Should be specified with total or pageCount\",\n    },\n    total: {\n      type: [String, Number],\n      default: 0,\n      description:\n        \"Can be specified instead of pageCount. The page count in this case will be total/perPage\",\n    },\n    value: {\n      type: Number,\n      default: 1,\n      description: \"Pagination value\",\n    },\n    size: {\n      type: String,\n      default: \"\",\n      description: \"Pagination size\",\n    },\n    align: {\n      type: String,\n      default: \"\",\n      description: \"Pagination alignment (e.g center|start|end)\",\n    },\n  },\n  computed: {\n    totalPages() {\n      if (this.pageCount > 0) return this.pageCount;\n      if (this.total > 0) {\n        return Math.ceil(this.total / this.perPage);\n      }\n      return 1;\n    },\n    pagesToDisplay() {\n      if (this.totalPages > 0 && this.totalPages < this.defaultPagesToDisplay) {\n        return this.totalPages;\n      }\n      return this.defaultPagesToDisplay;\n    },\n    minPage() {\n      if (this.value >= this.pagesToDisplay) {\n        const pagesToAdd = Math.floor(this.pagesToDisplay / 2);\n        const newMaxPage = pagesToAdd + this.value;\n        if (newMaxPage > this.totalPages) {\n          return this.totalPages - this.pagesToDisplay + 1;\n        }\n        return this.value - pagesToAdd;\n      } else {\n        return 1;\n      }\n    },\n    maxPage() {\n      if (this.value >= this.pagesToDisplay) {\n        const pagesToAdd = Math.floor(this.pagesToDisplay / 2);\n        const newMaxPage = pagesToAdd + this.value;\n        if (newMaxPage < this.totalPages) {\n          return newMaxPage;\n        } else {\n          return this.totalPages;\n        }\n      } else {\n        return this.pagesToDisplay;\n      }\n    },\n  },\n  data() {\n    return {\n      defaultPagesToDisplay: 5,\n    };\n  },\n  methods: {\n    range(min, max) {\n      let arr = [];\n      for (let i = min; i <= max; i++) {\n        arr.push(i);\n      }\n      return arr;\n    },\n    changePage(item) {\n      this.$emit(\"input\", item);\n    },\n    nextPage() {\n      if (this.value < this.totalPages) {\n        this.$emit(\"input\", this.value + 1);\n      }\n    },\n    prevPage() {\n      if (this.value > 1) {\n        this.$emit(\"input\", this.value - 1);\n      }\n    },\n  },\n  watch: {\n    perPage() {\n      this.$emit(\"input\", 1);\n    },\n    total() {\n      this.$emit(\"input\", 1);\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}